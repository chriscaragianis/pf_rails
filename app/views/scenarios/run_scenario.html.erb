<h1>Your Results</h1>

<table class="results">
  <thead>
    <tr>
      <th>Date</th>
      <th>Balance</th>
      <% Account.all.to_a.each do |acct| %>
        <th><%= acct.acct_name %></th>
        <th>Balance</th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% Scenario.find(16).balance_records.all.to_a.each do |br| %>
      <tr>
        <td><%= br.date %></td>
        <td><%= br.balance%></td>
        <td><%= br.balance%></td>
        <td><%= br.balance%></td>
      </tr>
    <% end %>
  </tbody>
</table>
        
 

